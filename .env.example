# Frontend Environment Variables
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here
VITE_API_URL=https://medical-rag-vector-uploader.onrender.com

# Backend Environment Variables
SUPABASE_URL=your_supabase_url_here
SUPABASE_KEY=your_supabase_service_role_key_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_JWT_SECRET=your_supabase_jwt_secret_here
PORT=8000

# Backend Debug Logging
BACKEND_DEBUG_LOGGING=true

# RunPod TxAgent Configuration
RUNPOD_EMBEDDING_URL=https://your-runpod-proxy-url.proxy.runpod.net
RUNPOD_EMBEDDING_KEY=your_runpod_api_key_here

# Phase 1 - Medical Consultation Configuration
TXAGENT_CONTAINER_URL=https://your-txagent-url.proxy.runpod.net
TXAGENT_TIMEOUT=120000

# Phase 1 - Basic Safety Features
ENABLE_EMERGENCY_DETECTION=true
MEDICAL_DISCLAIMER_REQUIRED=true

# Phase 1 - Logging Level
LOG_LEVEL=info

# Phase 2 - Voice Generation (ElevenLabs)
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
ELEVENLABS_VOICE_ID=default_voice_id

# Phase 2 - Enhanced Features
ENABLE_VOICE_GENERATION=true
ENABLE_SYMPTOM_TRACKING=true
ENABLE_TREATMENT_RECOMMENDATIONS=true

# External Services (optional fallbacks)
OPENAI_API_KEY=your_openai_api_key_here